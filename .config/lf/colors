# Colors for filenames in lf

# file types (with matching order)
ln      01;36   # LINK
or      31;01   # ORPHAN
tw      38;5;47 # STICKY_OTHER_WRITABLE
ow      38;5;47 # OTHER_WRITABLE
st      01;34   # STICKY
di      01;34   # DIR
pi      33      # FIFO
so      01;35   # SOCK
bd      33;01   # BLK
cd      33;01   # CHR
su      01;32   # SETUID
sg      01;32   # SETGID
ex      00   	# EXEC
fi      00      # FILE